  "scripts": {
    "start": "react-app-rewired start",
    "start:instrumented": "SKIP_PREFLIGHT_CHECK=true react-app-rewired -r @cypress/instrument-cra start",
    "build": "SKIP_PREFLIGHT_CHECK=true react-app-rewired build",
    "build:instrumented": "CYPRESS_INSTRUMENT_PRODUCTION=true react-app-rewired -r @cypress/instrument-cra build",
    "serve:instrumented": "serve -l 3000 -s ./build",
    "test": "react-app-rewired test --passWithNoTests",
    "test:coverage": "yarn test --watchAll=false --coverage",
    "test:component": "react-app-rewired test test.tsx",
    "test:unit": "react-app-rewired test spec.ts",
    "test:watch": "react-app-rewired test --watch",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src ./cypress",
    "lint:fix": "yarn lint --fix",
    "lint:fix:noWarnings": "yarn lint --fix --quiet",
    "lint:ts": "tsc --noEmit",
    "cy:coverage": "start-server-and-test 'yarn start:instrumented --watch=false' http://localhost:3000 'cy:ci'",
    "cy:ci": "cross-env CYPRESS_CREATE_COMPANY_BEFORE_RUN=true dotenv -- cypress run --spec 'cypress/integration/unit/*.spec.js' --browser chrome",
    "cy:qa": "dotenv -- cypress run --spec cypress/integration/qa/${TEST_ID}.spec.js --browser chrome",
    "cy:open": "dotenv cypress open",
    "report:coverage": "node ./scripts/coverage_merge.js",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "autolink": "node scripts/autolink.js",
    "autocopy": "node scripts/autocopy.js",
    "prepare": "husky install",
    "storybook": "start-storybook -s ./public -p 9001 --quiet",
    "storybook:build": "build-storybook -s ./public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
